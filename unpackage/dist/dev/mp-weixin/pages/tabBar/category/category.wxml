<view><view class="status" style="{{'position:'+(headerPosition)+';'}}"></view><view class="header" style="{{'position:'+(headerPosition)+';'}}"><view class="addr"><view class="icon location"></view>{{city}}</view><view class="input-box"><input placeholder="默认关键字" placeholder-style="color:#c0c0c0;" data-event-opts="{{[['tap',[['toSearch']]]]}}" bindtap="__e"/><view class="icon search"></view></view><view class="icon-btn"><view data-event-opts="{{[['tap',[['toMsg',['$event']]]]]}}" class="icon tongzhi" bindtap="__e"></view></view></view><view class="place"></view><view class="category-list"><scroll-view class="left" scroll-y="true"><block wx:for="{{categoryList}}" wx:for-item="category" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['showCategory',[index]]]]]}}" class="{{['row',index==showCategoryIndex?'on':'']}}" bindtap="__e"><view class="text"><view class="block"></view>{{''+category.title+''}}</view></view></block></scroll-view><scroll-view class="right" scroll-y="true"><block wx:for="{{categoryList}}" wx:for-item="category" wx:for-index="index" wx:key="id"><view hidden="{{!(index==showCategoryIndex)}}" class="category"><view class="list"><block wx:for="{{category.list}}" wx:for-item="row" wx:for-index="index1" wx:key="id"><view class="row"><view data-event-opts="{{[['tap',[['deleteGoods',['$0'],[[['categoryList','id',category.id],['list','id',row.id,'id']]]]]]]}}" class="menu" catchtap="__e"><view class="icon shanchu"></view></view><view data-event-opts="{{[['touchstart',[['touchStart',[index,'$event']]]],['touchmove',[['touchMove',[index,'$event']]]],['touchend',[['touchEnd',[index,'$event']]]]]}}" class="{{['carrier',theIndex==index?'open':oldIndex==index?'close':'']}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"><view data-event-opts="{{[['tap',[['selected',[index,index1]]]]]}}" class="checkbox-box" bindtap="__e"><view class="checkbox"><view class="{{[row.selected?'on':'']}}"></view></view></view><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['categoryList','id',category.id],['list','id',row.id]]]]]]]}}" class="goods-info" bindtap="__e"><view class="img"><image src="{{row.img}}"></image></view><view class="info"><view class="title">{{row.name}}</view><view class="spec">{{row.spec}}</view><view class="price-number"><view class="price">{{row.price}}</view></view></view></view></view></view></block></view></view></block></scroll-view></view></view>